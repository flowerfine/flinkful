<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sliew.flinkful.sql.catalog.repository.mapper.CatalogDatabaseMapper">

    <resultMap id="BaseResultMap" type="cn.sliew.flinkful.sql.catalog.repository.entity.CatalogDatabase">
        <result column="id" property="id" />
        <result column="creator" property="creator" />
        <result column="create_time" property="createTime" />
        <result column="editor" property="editor" />
        <result column="update_time" property="updateTime" />
        <result column="type" property="type" />
        <result column="catalog" property="catalog" />
        <result column="name" property="name" />
        <result column="properties" property="properties" />
        <result column="remark" property="remark" />
    </resultMap>

    <sql id="Base_Column_List">
        id, creator, create_time, editor, update_time,
        `type`, `catalog`, `name`, properties, remark
    </sql>

</mapper>
